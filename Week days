import java.util.*;

class Solution {
  public static void main(String[] args) {
    Scanner sc = new Scanner((System.in));
    System.out.println("Enter a number : ");
// from user taking input
    int k = sc.nextInt();
    switch (k) {
// Case check the given value with case number
      case 1:
        System.out.println("Sunday");
// Break is used to break the line if condition is true
        break;
      case 2:
        System.out.println("Monday");
        break;
      case 3:
        System.out.println("Tuesday");
        break;
      case 4:
        System.out.println("Wednesday");
        break;
      case 5:
        System.out.println("Thursday");
        break;
      case 6:
        System.out.println("Friday");
        break;
      case 7:
        System.out.println("Saturday");
        break;
// Default is used to print if all the statement is false3
      default:
        System.out.println("Error only upto 7 valid");
        break;
    }
  }
}
