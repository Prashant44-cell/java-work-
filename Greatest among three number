import java.util.*;

public class Solution {

  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
// Here three input is taken from user
    System.out.println("Enter First number");
    int a = sc.nextInt();
    System.out.println("Enter Second number");
    int b = sc.nextInt();
    System.out.println("Enter Third number");
    int c = sc.nextInt();
// Here condition are checked and along values are printed 
/* here condition are checked until it is statisfied and print along with the number 
*/
    if (a > b && b > c) {
      System.out.println("A is greatest that is " + a);
    } else if (b > a && b > c) {
      System.out.println("B is greatest that is " + b);
    } else {
      System.out.println("C is greatest that is " + c);
    }
  }
}
